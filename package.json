{
  "name": "tic-tac-toe-express",
  "version": "1.0.0",
  "description": "Создать приложение-игру крестики-нолики с поддержкой многопользовательской игры",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc -b",
    "start": "node dist/src/index.js",
    "lint": "eslint . && prettier --check .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vasiliev-alexey/tic-tac-toe-express.git"
  },
  "keywords": [
    "express soccket.io"
  ],
  "author": "vasiliev-alexey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vasiliev-alexey/cli_ghp_deployer"
  },
  "homepage": "https://github.com/vasiliev-alexey/cli_ghp_deployer#readme",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.0",
    "tslib": "^1.14.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,md,json,yml,html}": "prettier --write"
  }
}
