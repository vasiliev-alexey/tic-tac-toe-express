{
  "name": "tic-tac-toe-express",
  "version": "1.0.0",
  "description": "Создать приложение-игру крестики-нолики с поддержкой многопользовательской игры",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "webpack && tsc --project node_tsconfig.json",
    "build:prod": "webpack --mode production",
    "start": "node dist/src/backend/server.js",
    "lint": "eslint . && prettier --check .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"ts-node src/backend/server.ts\""
  },
  "proxy": "http://localhost:3000",
  "repository": {
    "type": "git",
    "url": "git@github.com:vasiliev-alexey/tic-tac-toe-express.git"
  },
  "keywords": [
    "express soccket.io"
  ],
  "author": "vasiliev-alexey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vasiliev-alexey/tic-tac-toe-express"
  },
  "homepage": "https://github.com/vasiliev-alexey/tic-tac-toe-express#readme",
