    "bootstrap": "^5.0.2",
    "bootswatch": "^5.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "memorable-moniker": "^0.5.0",
    "nanoid": "^3.1.23",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
